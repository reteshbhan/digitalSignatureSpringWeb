<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>3.1.2</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.digitalSignature</groupId>
	<artifactId>LoginFlow-2</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>LoginFlow-2</name>
	<description>Digital Signature take 2</description>
	<properties>
		<java.version>17</java.version>
	</properties>
	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-jpa</artifactId>
		</dependency>
		<!-- 
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-thymeleaf</artifactId>
		</dependency>
		-->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-devtools</artifactId>
			<scope>runtime</scope>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<scope>test</scope>
		</dependency>
		
		<!-- https://mvnrepository.com/artifact/org.apache.tomcat/tomcat-jasper -->
		<dependency>
		    <groupId>org.apache.tomcat</groupId>
		    <artifactId>tomcat-jasper</artifactId>
		    <version>10.1.11</version>
		</dependency>
		
		
		<!-- Added Dependenccy for JDBC-->
		<dependency>
		    <groupId>com.mysql</groupId>
		    <artifactId>mysql-connector-j</artifactId>
		    <version>8.0.33</version>
		</dependency>
		
		<dependency>
		   <groupId>org.springframework.boot</groupId>
		   <artifactId>spring-boot-starter-jdbc</artifactId>
		</dependency>
		
		<!-- https://mvnrepository.com/artifact/jstl/jstl -->
		<!-- https://mvnrepository.com/artifact/javax.servlet.jsp.jstl/jstl -->
		<!-- https://mvnrepository.com/artifact/javax.servlet/jstl -->
		
		<!-- https://mvnrepository.com/artifact/javax.servlet/jstl -->
		<dependency>
		    <groupId>javax.servlet</groupId>
		    <artifactId>jstl</artifactId>
		    <version>1.1.2</version>
		</dependency>
		
		<!-- PKCS11 Library 
		https://github.com/xipki/pkcs11wrapper/blob/sunpkcs11/README.md
		-->
		<dependency>
		    <groupId>org.xipki.iaik</groupId>
		    <artifactId>sunpkcs11-wrapper</artifactId>
		    <version>1.4.10</version>
		</dependency>
		
		<!-- https://jar-download.com/artifacts/org.xipki.iaik/sunpkcs11-wrapper/1.4.1/source-code/iaik/pkcs/pkcs11/Module.java -->
		<dependency>
			<groupId>org.xipki.iaik</groupId>
			<artifactId>sunpkcs11-wrapper</artifactId>
			<version>1.4.1</version>
		</dependency>
		<!-- Thanks for using https://jar-download.com -->

			
		<!-- PDFBox 
		http://www.java2s.com/Open-Source/Maven_Repository/PDF/pdfbox/pdfbox_0_7_3.htm
		-->
		<dependency>
		   <groupId>pdfbox</groupId>
		   <artifactId>pdfbox</artifactId>
		   <version>0.7.3</version>
		</dependency>
		
		<!-- MockMultipartFile jar to convert File to MultipartFile -->
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-test</artifactId>
			<version>3.1.1.RELEASE</version>
		</dependency>
		<!-- Thanks for using https://jar-download.com 
		https://jar-download.com/artifacts/org.springframework/spring-test/3.1.1.RELEASE/source-code/org/springframework/mock/web/MockMultipartFile.java-->


	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

</project>
